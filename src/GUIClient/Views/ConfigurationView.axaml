<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:GUIClient.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GUIClient.Views.ConfigurationView">
    <Design.DataContext>
        <viewModels:ConfigurationViewModel/>
    </Design.DataContext>
    <Grid RowDefinitions="Auto,Auto" ColumnDefinitions="Auto,*">
        <TextBlock Grid.Row="0" Grid.ColumnSpan="2" Classes="header2" Text="{Binding StrSystemConfigurations}" />
        
    </Grid>
</UserControl>
