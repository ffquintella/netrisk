<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <config>
        <add key="restoreIgnoreFailedSources" value="true" />
    </config>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
        <add key="uox-netrisk" value="https://nuget.cloudsmith.io/uox/netrisk/v3/index.json" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="ReactiveUI.Avalonia" />
            <package pattern="ReactiveUI.*" />
            <package pattern="Avalonia.*" />
            <package pattern="DynamicData" />
            <package pattern="Serilog.*" />
            <package pattern="System.*" />
            <package pattern="Microsoft.*" />
            <package pattern="SkiaSharp*" />
        </packageSource>
        <packageSource key="uox-netrisk">
            <package pattern="netrisk*" />
            <package pattern="uox*" />
        </packageSource>
    </packageSourceMapping>
</configuration>
