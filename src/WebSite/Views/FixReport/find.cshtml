@model FindFixRequestViewModel

@{
    Layout = "_Layout";
}


<h2>Find Fix Request</h2>

<form asp-action="Find" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="Id">ID:</label>
            <input asp-for="Id" class="form-control" style="margin: 5px 0">
        </div>
    

    <button type="submit" class="btn btn-primary">Find Fix Request</button>
</form>
