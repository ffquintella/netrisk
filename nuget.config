<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <config>
        <add key="restoreIgnoreFailedSources" value="true" />
    </config>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
        <add key="uox-netrisk" value="https://nuget.cloudsmith.io/uox/netrisk/v3/index.json" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="uox-netrisk">
            <package pattern="netrisk*" />
            <package pattern="OpenFaceONNX" />
            <package pattern="umapx" />
            <package pattern="FlashCap*" />
        </packageSource>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
    </packageSourceMapping>
</configuration>
